                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT R.[id]
	  , GETDATE() AS date
      ,R.[userid] AS 'user_id'
	  ,1 AS location_id
	  ,1 AS active
	  ,0 AS notified      
	  ,CAST(CONVERT(NVARCHAR, [startDate],112) AS DATETIME) AS checkIn
	  ,CAST(CONVERT(NVARCHAR, [endDate],112) AS DATETIME) AS checkOut
	  , DATEDIFF(DAY, [startDate], [endDate]) AS nights
      ,R.[total_charge] AS price     
  FROM [BejucoBeachHouse].[dbo].[Reservation] R
	LEFT JOIN [BejucoBeachHouse].[dbo].[User] U ON R.userid = U.id
	WHERE R.startDate &gt; GETDATE()</property>
