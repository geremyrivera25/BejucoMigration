                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT U.[id]
	  ,0 AS role
	  ,[state] As active
      ,[email]
      ,[full_name]
	  ,SUBSTRING([full_name], 1, CHARINDEX(' ', [full_name]) - 1) AS first_name     
      ,SUBSTRING([full_name], CHARINDEX(' ', [full_name]) + 1, LEN([full_name]) - CHARINDEX(' ', [full_name])) AS last_name
      ,REPLACE([phone],'-','') As phone
  FROM [BejucoBeachHouse].[dbo].[User] U
	Left JOIN [BejucoBeachHouse].[dbo].[Reservation] R ON U.id = R.userid
	WHERE R.startDate &gt; GETDATE()
GROUP BY U.id, U.email, U.full_name, u.state,  u.role, u.personal_id, u.phone</property>
