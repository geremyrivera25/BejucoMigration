                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select a.id
	  ,a.[state] as active
	  ,0 AS role    
      ,FirstName = Pos1+case when Pos3 is not null then ' 'else '' end
      ,LastName  = case when Pos3 is null then Pos2 else Pos3 end
      ,email = case when Pos3 is null then LOWER(pos1)+'.'+Lower(pos2)+'@3pillarglobal.com' else LOWER(pos1)+'.'+Lower(pos3)+'@3pillarglobal.com' end
	  ,REPLACE(a.[phone],'-','') As phone
 From dbo.[User] A
 Cross Apply (
    Select Pos1 = xDim.value('/x[1]','varchar(max)')
          ,Pos2 = xDim.value('/x[2]','varchar(max)')
          ,Pos3 = xDim.value('/x[3]','varchar(max)')
          ,Pos4 = xDim.value('/x[4]','varchar(max)')
     From  (Select Cast('&lt;x&gt;' + replace((Select substring(full_name,1,charindex(' and ',full_name+' and ')-1) as [*] For XML Path('')),' ','&lt;/x&gt;&lt;x&gt;')+'&lt;/x&gt;' as xml) as xDim) as A 
 ) B Left JOIN [dbo].[Reservation] R ON a.id = R.userid
    WHERE R.startDate &gt; GETDATE()
GROUP BY a.id, a.email, a.full_name, a.state, a.role, a.phone, B.Pos1, B.Pos2, B.Pos3</property>
